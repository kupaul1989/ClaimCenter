<?xml version="1.0"?>
<!--suppress XmlUnusedNamespaceDeclaration -->
<xs:schema
        targetNamespace="http://guidewire.com/ws/soapheaders_ref"
        xmlns:gwsoap="http://guidewire.com/ws/soapheaders"
        xmlns:gwsoapref="http://guidewire.com/ws/soapheaders_ref"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        elementFormDefault="qualified">

  <xs:import namespace="http://guidewire.com/ws/soapheaders" schemaLocation="soapheaders.xsd"/>

  <xs:element name="guidewire">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="gwsoap:authentication"/>
        <xs:element ref="gwsoap:locale">
          <xs:annotation>
            <xs:documentation>
              &lt;b&gt;Deprecated&lt;/b&gt;. With Guidewire products -- gw_language and gw_locale should be used.
            </xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="gwsoap:gw_locale"/>
        <xs:element ref="gwsoap:gw_language"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

</xs:schema>
